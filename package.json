{
  "name": "quiz",
  "version": "1.3.1",
  "description": "A REST API for quiz app",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "preversion": "git-branch-is -r \"^(feat|fix|hotfix)/CLO-\\d+/[a-z\\-\\d\\.]+$\" && echo Preversion checks passed. Your branch name is correct format.",
    "prepare": "husky install",
    "typeorm": "node -r dotenv/config -r tsconfig-paths/register -r ts-node/register ./node_modules/typeorm/cli.js -d ./src/configs/orm-migrations.ts",
    "migration:generate": "sh -c 'yarn typeorm migration:generate ./src/migrations/$0'",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "release": "standard-version --no-verify && git push --no-verify"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.0.11",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/platform-socket.io": "^9.4.3",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.4.3",
    "@nestjsx/crud-request": "^5.0.0-alpha.3",
    "@supercharge/request-ip": "^1.2.0",
    "@types/lodash": "^4.14.182",
    "axios": "^0.27.2",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.4",
    "cache-manager": "^4.1.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "csvtojson": "^2.0.10",
    "device-detector-js": "^3.0.3",
    "dotenv": "^16.0.1",
    "express-basic-auth": "^1.2.1",
    "ioredis": "^5.2.2",
    "jwks-rsa": "^2.1.4",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "nanoid": "^3.3.1",
    "nodemailer-mailgun-transport": "^2.1.5",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.7",
    "typeorm-naming-strategies": "^4.1.0",
    "web3": "^1.7.5",
    "web3-eth-contract": "^1.7.5",
    "web3-utils": "^1.7.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nestjs-architects/typed-cqrs": "^1.0.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.11",
    "@types/cache-manager": "^4.0.1",
    "@types/cache-manager-ioredis": "^2.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.7",
    "@types/node": "^18.7.5",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-branch-is": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "standard-version": "^9.5.0",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  }
}
